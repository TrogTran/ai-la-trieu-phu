               <property name="styleSheet">
                <string notr="true">/* QLabel - Tiêu đề User Name, Password */
QLabel {
    font-size: 18px;
    font-weight: bold;
    color: #FFD700; /* Vàng nổi bật */
    text-shadow: 2px 2px 4px #000;
    background: transparent;
}

/* QLineEdit - Ô nhập username, password */
QLineEdit {
    font-size: 16px;
    padding: 8px;
    border: 2px solid #0077CC; /* Viền xanh neon */
    border-radius: 8px;
    background: rgba(0, 0, 50, 0.8); /* Màu xanh đậm trong suốt */
    color: white;
}

/* Khi nhập vào ô, viền sẽ sáng lên */
QLineEdit:focus {
    border: 2px solid #00CFFF; 
    background: rgba(0, 0, 80, 0.9);
}

/* QPushButton - Nút Đăng nhập và Đăng ký */
QPushButton {
    font-size: 18px;
    font-weight: bold;
    padding: 10px;
    border-radius: 10px;
    color: white;
    border: 2px solid #FFD700; /* Viền vàng nổi bật */
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                                stop:0 #0057D9, stop:1 #002F7A);
    transition: all 0.3s ease-in-out;
}

/* Hiệu ứng hover - Sáng rực hơn khi rê chuột */
QPushButton:hover {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                                stop:0 #007AFF, stop:1 #0044AA);
    border-color: #FFFFFF;
}

/* Hiệu ứng khi nhấn nút - Làm tối lại để giống hiệu ứng bấm trong game */
QPushButton:pressed {
    background: qlineargradient(x1:0, y1:0, x2:1, y2:1,
                                stop:0 #003C82, stop:1 #001A40);
    border-color: #FFD700;
}
</string>
