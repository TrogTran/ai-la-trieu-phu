     <string notr="true">QPushButton {
    font-size: 18px;
    font-weight: bold;
    color: #FFD700; /* Màu vàng */
    background-color: #003366; /* Xanh đậm */
    border: 3px solid #FFD700; /* Viền vàng */
    border-radius: 25px; /* Bo tròn */
    padding: 10px 20px;
    min-width: 120px;
    text-transform: uppercase;
    letter-spacing: 1px;
    outline: none;
    transition: all 0.3s ease-in-out;
}

/* Hiệu ứng hover - Nút sáng lên */
QPushButton:hover {
    background-color: #0044aa; /* Xanh sáng hơn */
    border: 3px solid #FFEE58; /* Viền vàng sáng */
    box-shadow: 0px 0px 10px rgba(255, 215, 0, 0.8);
}

/* Hiệu ứng khi nhấn */
QPushButton:pressed {
    background-color: #002244; /* Xanh tối */
    border: 3px solid #FFCC00; /* Viền vàng đậm */
    box-shadow: 0px 0px 5px rgba(255, 215, 0, 0.5);
    transform: scale(0.95);
}

/* Nút bị vô hiệu hóa */
QPushButton:disabled {
    background-color: #555555; /* Xám */
    border: 3px solid #777777;
    color: #999999;
}
